{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "schematics": {
    "schematics": {
      "@nestjs/schematics:component": {
        "spec": false
      },
      "@nestjs/schematics:service": {
        "spec": false
      },
      "@nestjs/schematics:controller": {
        "spec": false
      }
    }
  }
}
